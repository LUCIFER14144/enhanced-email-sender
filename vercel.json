{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/admin/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "admin/static/$1"
    },
    {
      "src": "/",
      "dest": "api/main.py"
    }
  ],
  "functions": {
    "api/main.py": {
      "maxDuration": 30
    }
  }
}
