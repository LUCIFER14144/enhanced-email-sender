{
  "version": 2,
  "builds": [
    { "src": "api/main.py", "use": "@vercel/python" },
    { "src": "admin/static/**", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/download",
      "dest": "/admin/static/download.html"
    },
    {
      "src": "/static/(.*)",
      "dest": "/admin/static/$1"
    },
    {
      "src": "/admin",
      "dest": "/api/main.py"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/api/main.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/main.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  },
  "build": {
    "env": {
      "PIP_TARGET": ".pip",
      "PYTHONPATH": "/var/task/.pip"
    }
  }
}
